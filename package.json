{
  "name": "comunica-experiments",
  "version": "0.0.0",
  "private": true,
  "description": "Testing things with Comunica",
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "query": "node tools/query-runner/bin/query-runner.js",
    "index": "node tools/index-generator/bin/index-generator.js",
    "generate": "NODE_OPTIONS=--max-old-space-size=8192 solidbench generate",
    "serve": "solidbench serve",
    "clean": "rm -rf ./out-*"
  },
  "author": "surilindur",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "componentsjs-generator": "^3.1.0",
    "eslint": "^8.27.0",
    "solidbench": "^1.4.1",
    "typescript": "^4.8.4"
  }
}
