{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "indent": "off",
    "@typescript-eslint/indent": [
      "error",
      2
    ]
  },
  "ignorePatterns": [
    "**/*.d.ts"
  ],
  "root": true
}